    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "date-fns": "^2.15.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.2",
    "electron-overlay-window": "^1.0.2",
    "electron-store": "^6.0.0",
    "electron-updater": "^4.3.1",
    "events": "^3.2.0",
    "node-loader": "^1.0.1",
    "polka": "^0.5.2",
    "sirv": "^1.0.5",
    "uiohook-napi": "^1.0.7",
    "ws": "^7.3.1"
  }
