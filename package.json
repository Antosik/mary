{
  "name": "mary",
  "version": "0.0.1-prerelease.5",
  "description": "",
  "main": "target/app/main.js",
  "scripts": {
    "clean": "del-cli ./target",
    "compile": "npm run clean && npm run compile:main && npm run compile:renderer",
    "compile:main": "cross-env NODE_ENV=\"production\" webpack --config main/webpack.config.ts",
    "compile:renderer": "cross-env NODE_ENV=\"production\" webpack --config renderer/webpack.config.ts",
    "dist:linux": "electron-builder --config ./electron-builder.json --linux",
    "dist:mac": "electron-builder --config ./electron-builder.json --mac",
    "dist:windows": "electron-builder --config ./electron-builder.json --win",
    "lint": "concurrently -n \"ESLint,CSSLint,Svelte\" -c \"cyan.bold,blue.bold,green.bold\" \"npm run lint:js\" \"npm run lint:css\" \"npm run lint:svelte\"",
    "lint:js": "eslint --ext .js,.ts .",
    "lint:css": "stylelint renderer/**/*.{css,html,svelte}",
    "lint:svelte": "svelte-check --workspace renderer",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "npm run compile",
    "start:dev": "npm run clean && concurrently -n \"WebpackMain,WebpackRenderer\" -c \"cyan.bold,blue.bold\" \"webpack -w --config main/webpack.config.ts\" \"webpack -w --config renderer/webpack.config.ts\" -k",
    "start:app": "electron ."
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^10.17.28",
    "@types/polka": "^0.5.1",
    "@types/terser-webpack-plugin": "^3.0.0",
    "@types/webpack": "^4.41.21",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "del-cli": "^3.0.1",
    "electron": "^9.1.2",
    "electron-builder": "^22.7.0",
    "eslint": "^7.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^4.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.24.0",
    "svelte-check": "^1.0.5",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.0.9",
    "terser-webpack-plugin": "^3.0.8",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "Antosik",
  "license": "MIT",
